   "source": [
    "sayi = 9\n",
    "asal = True\n",
    "\n",
    "if sayi > 1:\n",
    "    i = 2\n",
    "    while i * i <= sayi:\n",
    "        if sayi % i == 0:\n",
    "            asal = False\n",
    "            break\n",
    "        i += 1\n",
    "else:\n",
    "    asal = False\n",
    "\n",
    "if asal:\n",
    "    print(sayi, \"sayısı asaldır.\")\n",
    "else:\n",
    "    print(sayi, \"sayısı asal değildir.\")\n"
   ]
  }
